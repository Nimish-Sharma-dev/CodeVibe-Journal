{
  "name": "dev-companion-backend",
  "version": "1.0.0",
  "description": "AI-powered developer companion platform backend with GitHub repo analysis, daily logging, and activity tracking",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
